@prefix ex: <http://example/heart#>.

#1
{
 ?o1 a ex:Observation ; 
     ex:hasFactor ex:MR ; 
     ex:hasObsValue "organic" ;
     ex:hasPatient ?p .	

 ?o2 a ex:Observation ; 
     ex:hasFactor ex:Disease ; 
     ex:hasObsValue "degenerative" ;
     ex:hasPatient ?p .	

 ?o3 a ex:Observation ; 
     ex:hasFactor ex:valveLeaflets ; 
     ex:hasObsValue "normal" ;
     ex:hasPatient ?p 
} => {
	_:bnode a ex:Observation ; 
		ex:hasFactor ex:RingRecommendation ; 
     		ex:RingSize "normal" ;
     		ex:RingModels "B", "C" ;
		ex:hasPatient ?p .
      } .

#2
{
 ?o1 a ex:Observation ; 
     ex:hasFactor ex:MR ; 
     ex:hasObsValue "organic" ;
     ex:hasPatient ?p .	

 ?o2 a ex:Observation ; 
     ex:hasFactor ex:Disease ; 
     ex:hasObsValue "degenerative" ;
     ex:hasPatient ?p .	

 ?o3 a ex:Observation ; 
     ex:hasFactor ex:valveLeaflets ; 
     ex:hasObsValue "elongated" ;
     ex:hasPatient ?p 
} => {
	_:bnode a ex:Observation ; 
		ex:hasFactor ex:RingRecommendation ; 
     		ex:RingSize "large" ;
     		ex:RingModels "A" ;
		ex:hasPatient ?p .
      } .	

#3
{
 ?o1 a ex:Observation ; 
     ex:hasFactor ex:MR ; 
     ex:hasObsValue "organic" ;
     ex:hasPatient ?p .	

 ?o2 a ex:Observation ; 
     ex:hasFactor ex:Disease ; 
     ex:hasObsValue "non-degenerative" ;
     ex:hasPatient ?p .	

 ?o3 a ex:Observation ; 
     ex:hasFactor ex:valveLeaflets ; 
     ex:hasObsValue "normal" ;
     ex:hasPatient ?p .

######## Optional #################################################################
# ?o4 a ex:Observation ;                                                          #
#     ex:hasFactor ex:subDisease ;                                                #
#     ex:hasObsValue "endocarditis" OR "rheumatic valve" OR "congenital lesions" ;#
#     ex:hasPatient ?p                                                            #
###################################################################################

} => {
	_:bnode a ex:Observation ; 
		ex:hasFactor ex:RingRecommendation ; 
     		ex:RingSize "normal" ;
     		ex:RingModels "B", "D", "E" ;
		ex:hasPatient ?p .
      } .


#4
{
 ?o1 a ex:Observation ; 
     ex:hasFactor ex:MR ; 
     ex:hasObsValue "functional" ;
     ex:hasPatient ?p .	

 ?o2 a ex:Observation ; 
     ex:hasFactor ex:ischemic ; 
     ex:hasObsValue "yes" ;
     ex:hasPatient ?p 

} => {
	_:bnode a ex:Observation ; 
		ex:hasFactor ex:RingRecommendation ; 
     		ex:RingSize "small, complete rigid" ;
     		ex:RingModels "F" ;
		ex:hasPatient ?p .
      } .	


#5
{
 ?o1 a ex:Observation ; 
     ex:hasFactor ex:MR ; 
     ex:hasObsValue "functional" ;
     ex:hasPatient ?p .	

 ?o2 a ex:Observation ; 
     ex:hasFactor ex:dilated ; 
     ex:hasObsValue "yes" ;
     ex:hasPatient ?p 
} => {
	_:bnode a ex:Observation ; 
		ex:hasFactor ex:RingRecommendation ; 
     		ex:RingSize "small, complete rigid" ;
     		ex:RingModels "G" ;
		ex:hasPatient ?p .
      } .

# A - Myxo Etlogix annuloplasty ring (Edwards Lifescience)
# B - Carpentier-Edwards Classic mitral annuloplasty ring (Edwards Lifescience)
# C - Colvin-Galloway Future Composite Ring (Medtronic, Minneapolis, Minn)
# D - Rigid Saddle Ring (St. Jude Medical, St. Paul, Minn)
# E - AnnuloFlo, annuloplasty ring (CarboMedics/Sorin Group, Arvada, Colo) 
# F - Carpentier-McCarthy-Adams IMR Etlogix ring (Edwards Lifescience)
# G - GeoForm annuloplasty ring (Edwards Lifescience)